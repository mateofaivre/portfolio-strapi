  "options": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "content": {
      "type": "customField",
      "options": {
        "preset": "toolbarBaloon"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "link": {
      "type": "component",
      "repeatable": false,
      "component": "default.link"
    },
    "medias": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "audios"
      ]
    },
    "carousel_pagination_color": {
      "type": "enumeration",
